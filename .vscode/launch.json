    "version": "0.2.0",
    "configurations": [

        {
            "name": "inpaint",
            "type": "debugpy",
            "request": "launch",
            "program": "scripts/inpaint.py",
            "console": "integratedTerminal",
            "args": [
                "--indir",
                "data/inpainting_examples/",
                "--outdir",
                "outputs/inpainting_results",
            ],
        },
        {
            "name": "main",
            "type": "debugpy",
            "request": "launch",
            "program": "./main.py",
            "console": "integratedTerminal",
            "env": {
                "CUDA_VISIBLE_DEVICES": "0"
            },
            "args": [
                "--base",
                "configs/latent-diffusion/dino_ddpm_single.yaml",
                "-t",
                "--gpus",
                "0,",
            ],
        },
    ]
}